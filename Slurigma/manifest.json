{
	"manifest_version": 2,
	"name": "Slurigma",
	"version": "0.0.2",
	"description": "Slur Machine Extenstion",
	"permissions": [
	"storage",
	"contextMenus",
	"tabs",
	"storage",
	"http://www.slurmachine.fun/*" ],
	"background": {
		"scripts": [
		"jquery.js",
		"background.js",
		"methods.js"],
		"persistent": true
		},
	"icons": { "16": "slur16.png",
			   "48": "slur48.png",
			  "128": "slur128.png" }

}
